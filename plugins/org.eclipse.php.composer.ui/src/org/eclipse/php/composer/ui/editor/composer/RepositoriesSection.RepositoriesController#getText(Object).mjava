		@Override
		public String getText(Object element) {
			if (element instanceof Repository) {
				Repository repo = (Repository)element;
				String name;
				
				// name
				if (repo.has("name")) {
					name = repo.getAsString("name");
				} else {
					name = repo.getUrl();
				}
				
				return name;
			}

			return super.getText(element);
		}

