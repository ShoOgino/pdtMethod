		public int compare(Object o1, Object o2) {
			TextEditBasedChangeGroup c1 = (TextEditBasedChangeGroup) o1;
			TextEditBasedChangeGroup c2 = (TextEditBasedChangeGroup) o2;
			int p1 = getOffset(c1);
			int p2 = getOffset(c2);
			if (p1 < p2)
				return -1;
			if (p1 > p2)
				return 1;
			// same offset
			return 0;
		}

