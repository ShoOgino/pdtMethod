	private void loadSettings() {
		fSettings = getDialogSettings().getSection(
				ExtractFunctionWizard.DIALOG_SETTING_SECTION);
		if (fSettings == null) {
			fSettings = getDialogSettings().addNewSection(
					ExtractFunctionWizard.DIALOG_SETTING_SECTION);
			fSettings.put(GENERATE_PHPDOC, true);
		}
	}

