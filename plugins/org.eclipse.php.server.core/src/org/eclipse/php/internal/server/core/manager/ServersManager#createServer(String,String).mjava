	/**
	 * Creates and adds a server.
	 * 
	 * @param name
	 * @param baseURL
	 * @return
	 * @throws MalformedURLException
	 */
	public static Server createServer(String name, String baseURL)
			throws MalformedURLException {
		Server server = new Server(name, "localhost", baseURL, "");
		addServer(server);
		return server;
	}

