	public Map<String, Object> storeToMap() {
		Map<String, Object> properties = new HashMap<String, Object>(helper.map);
		Map<String, Object> serverMap = new HashMap<String, Object>(1);
		serverMap.put(SERVER_ELEMENT, properties);
		return serverMap;
	}

