	/**
	 * Returns whether the property is a number.
	 * 
	 * @return
	 *         <ul>
	 *         <li><code>true</code> property is a number</li>
	 *         <li><code>false</code> property is not a number</li>
	 *         </ul>
	 */
	public boolean isNumber() {
		return value instanceof Number;
	}

