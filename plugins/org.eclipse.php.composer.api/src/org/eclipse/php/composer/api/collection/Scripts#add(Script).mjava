	/**
	 * Adds a new dependency.
	 * 
	 * @param dependency
	 *            the new dependency
	 * @return this
	 */
	public void add(Script script) {
		if (has(script)) {
			get(script.getScript()).addHandlers(script.getHandlers());
		} else {
			script.addPropertyChangeListener(listener);
			set(script.getScript(), script);
		}
	}

