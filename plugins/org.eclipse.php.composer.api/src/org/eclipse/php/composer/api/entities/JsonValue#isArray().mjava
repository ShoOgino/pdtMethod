	/**
	 * Returns whether the value is instance of an array.
	 * 
	 * @see #getAsArray
	 * @return
	 *         <ul>
	 *         <li><code>true</code> property is an array</li>
	 *         <li><code>false</code> property is not an array</li>
	 *         </ul>
	 */
	public boolean isArray() {
		return value instanceof JsonArray;
	}

