	/**
	 * Constructs profiler db from the specified profiler data. May be useful
	 * when restoring profiler db from file.
	 * 
	 * @param ProfilerData
	 *            data
	 * @param Date
	 *            date
	 */
	public DefaultProfilerDB(ProfilerData data, Date date) {
		fProfilerData = data;
		fProfileDate = date;
		buildFilesHashtable();
		buildFunctionsHashtable();
	}

