		/*
		 * (non-Javadoc)
		 * 
		 * @see
		 * org.eclipse.jface.viewers.ITableLabelProvider#getColumnImage(java
		 * .lang.Object, int)
		 */
		public Image getColumnImage(Object element, int columnIndex) {
			if (element instanceof InvokeFunctionTableElement) {
				switch (columnIndex) {
				case 1:
					return fFunctionImage;
				case 2:
					return fFileImage;
				}
			}
			return null;
		}

