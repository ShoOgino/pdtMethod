	@Override
	public boolean hasChildren(final Object element) {
		if (element instanceof PHPUnitTestCase) {
			final PHPUnitTestException exception = ((PHPUnitTestCase) element).getException();
			if (exception == null) {
				return false;
			}
			diff = exception.getDiff();
			return diff != null && !diff.isEmpty();
		} else if (element instanceof DiffLine) {
			return !((DiffLine) element).getChildren().isEmpty();

		}
		return false;
	}

