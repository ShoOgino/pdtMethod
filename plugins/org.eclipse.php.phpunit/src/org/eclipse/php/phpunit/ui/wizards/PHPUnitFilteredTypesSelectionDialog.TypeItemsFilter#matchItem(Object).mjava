		@Override
		public boolean matchItem(Object item) {
			if (fMatchEverything) {
				return true;
			}

			TypeNameMatch type = (TypeNameMatch) item;
			if (!(matchesPackage(type) && matchesModifiers(type) && matchesScope(type) && matchesFilterExtension(type)))
				return false;
			return matchesName(type);
		}

