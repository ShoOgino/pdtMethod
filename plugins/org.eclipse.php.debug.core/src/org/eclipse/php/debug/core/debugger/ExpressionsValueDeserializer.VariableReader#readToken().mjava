        private String readToken() {
            StringBuffer buffer = new StringBuffer(6);
            char curr;
            do {
                curr = (char) super.read();
            } while (curr == ';' || curr == ':');

            while (curr != ';' && curr != ':') {
                buffer.append(curr);
                curr = (char) super.read();
            }
            return buffer.toString();
        }

