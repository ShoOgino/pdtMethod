	/**
	 * Called when this debug target terminates.
	 */
	public void terminated() {
		fTerminated = true;
		fSuspended = false;
		Logger.debugMSG("[" + this + "] PHPDebugTarget: Calling debugger.closeConnection();");
		if (!fTermainateCalled) {
			debugger.closeConnection();
		}
		completeTerminated();
	}

