    public boolean equals(Object obj) {
        if (super.equals(obj)) {
            return true;
        }
        if (obj == null || !(obj instanceof Breakpoint)) {
            return false;
        }
        Breakpoint other = (Breakpoint) obj;
        return this.id == other.id && this.type == other.type && this.lifetime == other.lifetime && fileName.equals(other.getFileName()) && lineNumber == other.getLineNumber();
    }

