	private String[] mapAsArray(Map map) {
		String[] strArr = new String[map.size()];
		Iterator entries = map.entrySet().iterator();
		int index = 0;
		while (entries.hasNext()) {
			Entry entry = (Entry) entries.next();
			strArr[index++] = entry.getKey() + "=" + entry.getValue();
		}
		return strArr;
	}

