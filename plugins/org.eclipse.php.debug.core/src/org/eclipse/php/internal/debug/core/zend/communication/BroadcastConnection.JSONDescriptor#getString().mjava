		public String getString() {
			try {
				Iterator<String> keys = properties.keySet().iterator();
				StringBuffer sb = new StringBuffer("{"); //$NON-NLS-1$
				while (keys.hasNext()) {
					if (sb.length() > 1) {
						sb.append(',');
					}
					Object o = keys.next();
					sb.append(quote(o.toString()));
					sb.append(':');
					sb.append(properties.get(o));
				}
				sb.append('}');
				return sb.toString();
			} catch (Exception e) {
				return null;
			}
		}

