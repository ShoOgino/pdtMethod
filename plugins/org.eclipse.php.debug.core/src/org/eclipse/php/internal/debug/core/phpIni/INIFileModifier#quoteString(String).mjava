	/**
	 * Puts quotes around the given string
	 * 
	 * @param str
	 *            String
	 * @return result string
	 */
	private String quoteString(String str) {
		if (str.startsWith("\"") && str.endsWith("\"") || str.startsWith("'")
				&& str.endsWith("'")) {
			return str;
		}
		return '"' + str + '"';
	}

