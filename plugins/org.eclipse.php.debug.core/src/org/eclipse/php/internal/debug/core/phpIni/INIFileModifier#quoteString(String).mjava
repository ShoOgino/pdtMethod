	/**
	 * Puts quotes around the given string
	 * 
	 * @param str
	 *            String
	 * @return result string
	 */
	private String quoteString(String str) {
		if (str.startsWith("\"") && str.endsWith("\"") || str.startsWith("'") //$NON-NLS-1$ //$NON-NLS-2$ //$NON-NLS-3$
				&& str.endsWith("'")) { //$NON-NLS-1$
			return str;
		}
		return '"' + str + '"';
	}

