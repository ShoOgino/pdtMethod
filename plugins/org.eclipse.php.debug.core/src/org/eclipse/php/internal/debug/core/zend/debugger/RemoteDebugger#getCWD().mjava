	public String getCWD() {
		if (!this.isActive()) {
			return null;
		}

		PHPDebugTarget debugTarget = getDebugHandler().getDebugTarget();
		int suspendCount = debugTarget.getSuspendCount();
		if (suspendCount == previousSuspendCount && cachedCWD != null) {
			return cachedCWD;
		}

		System.out.println("GETCWD");
		if (canDo(GET_CWD)) {
			cachedCWD = getCWDNew();
		} else {
			cachedCWD = getCWDOld();
		}
		return cachedCWD;
	}

