	private static int[] three2four(byte a, byte b, byte c) {

		/*
		 aaaaaa|aabbbb|bbbbcc|cccccc
		 */
		int[] result = new int[4];
		result[0] = a >>> 2 & 0x3f;
		result[1] = ((a & 0x03) << 4) | ((b & 0xf0) >>> 4);
		result[2] = ((b & 0x0f) << 2) | ((c & 0xc0) >>> 6);
		result[3] = c & 0x3f;
		return result;

	}

