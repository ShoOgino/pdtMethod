	void genValueString(String data) {
		stringInfo = null;
		if (data != null && data.trim().length() > 0) {
			setValueString(data);
		} else {
			// if we have no data, then we have an empty string. Could confirm this
			// with the length.
			setValueString("");
			setValueBytes(new byte[0]);
		}
	}

