	/*
	 * (non-Javadoc)
	 * 
	 * @see org.eclipse.debug.core.model.IStep#stepOver()
	 */
	public void stepOver() throws DebugException {
		synchronized (commandMutex) {
			if (!canStepOver())
				return;
			stepping = true;
			resumed(DebugEvent.STEP_OVER);
		}
		session.sendAsyncCmd(DBGpCommand.stepOver);

	}

