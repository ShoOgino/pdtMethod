		@Override
		protected IStatus run(IProgressMonitor monitor) {
			switch (actionType) {
			case HANDLE_BREAK: {
				handleBreak();
				break;
			}
			case HANDLE_STOPPING: {
				handleStopping();
				break;
			}
			case HANDLE_STOP: {
				handleStop();
				break;
			}
			case HANDLE_STREAM: {
				handleStream();
				break;
			}
			default:
				break;
			}
			return Status.OK_STATUS;
		}

