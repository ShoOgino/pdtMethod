	/*
	 * (non-Javadoc)
	 * 
	 * @see org.eclipse.debug.core.model.IStep#stepInto()
	 */
	@Override
	public void stepInto() throws DebugException {
		synchronized (commandMutex) {
			if (!canStepInto())
				return;
			stepping = true;
			resumed(DebugEvent.STEP_INTO);
		}
		session.sendAsyncCmd(DBGpCommand.stepInto);
	}

