	/**
	 * Creates new DBGp variable.
	 * 
	 * @param target
	 * @param stackLevel
	 * @param fullName
	 * @param facets
	 */
	public AbstractDBGpBaseVariable(IDebugTarget target, String stackLevel,
			String fullName, Facet... facets) {
		super(target);
		this.stackLevel = stackLevel;
		this.fullName = fullName;
		addFacets(facets);
	}

