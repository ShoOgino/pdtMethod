	/**
	 * end this session
	 *
	 */
	public void endSession() {
		try {
			DBGpSocket.close();
			socketClosed = true;
			if (debugTarget != null) {
				debugTarget.sessionEnded();
			}
		} catch (IOException e) {
			// Ignore the exception except for debug purposes
			DBGpLogger.debugException(e);
		}
	}

