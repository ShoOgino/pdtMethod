	/*
	 * (non-Javadoc)
	 * 
	 * @see org.eclipse.debug.core.model.IStep#stepReturn()
	 */
	@Override
	public void stepReturn() throws DebugException {
		synchronized (commandMutex) {
			if (!canStepReturn()) {
				return;
			}
			stepping = true;
			resumed(DebugEvent.STEP_RETURN);
		}
		session.sendAsyncCmd(DBGpCommand.StepOut);
	}

