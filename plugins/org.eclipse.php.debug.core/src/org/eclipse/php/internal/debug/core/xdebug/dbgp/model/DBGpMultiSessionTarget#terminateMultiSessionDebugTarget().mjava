	private void terminateMultiSessionDebugTarget() {
		if (webLaunch) {
			sendStopDebugURL();
		}
		targetState = STATE_TERMINATED;
		DebugPlugin.getDefault().removeDebugEventListener(this);
		fireTerminateEvent();
		// Terminate corresponding launch as well
		try {
			getLaunch().terminate();
		} catch (DebugException e) {
		}
	}

