	public Expression[] getStackVariables() throws DebugException {
		IStackFrame frame = getTopStackFrame();
		if (frame == null) {
			return NO_VARIABLES;
		}
		Expression[] stackVariables = ((PHPStackFrame) frame)
				.getStackVariables();
		if (stackVariables == null) {
			return NO_VARIABLES;
		}
		return stackVariables;
	}

