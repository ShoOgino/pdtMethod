	private void copyVariablesFromPreviousFrames(IStackFrame[] frames) {
		if (fFrames != null) {
			for (int i = frames.length - 1, c = fFrames.length - 1; i > 0
					&& c >= 0; --i, --c) {
				((PHPStackFrame) frames[i])
						.setStackVariables(((PHPStackFrame) fFrames[c])
								.getStackVariables());
			}
		}
	}

