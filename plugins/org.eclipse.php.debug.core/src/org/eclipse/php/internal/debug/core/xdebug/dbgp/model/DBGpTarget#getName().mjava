	/*
	 * (non-Javadoc)
	 * 
	 * @see org.eclipse.debug.core.model.IDebugTarget#getName()
	 */
	public String getName() throws DebugException {
		if (isWebLaunch() || multiSessionManaged) {
			return "Remote Launch";
		} else {
			if (scriptName == null) {
				return "Unknown PHP Program";
			}
		}
		return scriptName;
	}

