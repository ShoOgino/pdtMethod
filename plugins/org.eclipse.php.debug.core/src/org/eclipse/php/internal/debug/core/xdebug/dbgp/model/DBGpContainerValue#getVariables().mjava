	/*
	 * (non-Javadoc)
	 * @see org.eclipse.php.xdebug.core.dbgp.model.DBGpValue#getVariables()
	 */
	public IVariable[] getVariables() throws DebugException {
		if (childVariables == null) {
			requestValue();
			if (childVariables == null) {
				childVariables = new IVariable[0];
			}
		}
		return childVariables;
	}

