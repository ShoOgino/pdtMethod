	public void deserialize(DataInputStream in) throws IOException {
		setID(in.readInt());
		int pathSize = in.readInt();
		if (pathSize > 0) {
			String[] paths = new String[pathSize];
			for (int i = 0; i < pathSize; i++) {
				paths[i] = CommunicationUtilities.readString(in);
			}
			setPaths(paths);
		}
	}

