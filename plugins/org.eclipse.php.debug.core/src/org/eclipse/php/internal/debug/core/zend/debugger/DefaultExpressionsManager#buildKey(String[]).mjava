	private static String buildKey(String[] name) {
		StringBuffer buffer = new StringBuffer(name.length * 5); // 5 as the
																	// average
																	// size of
																	// variable
																	// name
		for (int i = 0; i < name.length; i++) {
			buffer.append(name[i]);
			buffer.append(' ');
		}
		return buffer.toString();
	}

