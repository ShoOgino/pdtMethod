		protected void fieldModified() {
			final String text = fNumberText.getText();
			final String errorText = fInputValidator != null ? fInputValidator.isValid(text) : null;
			if (errorText == null) {
				updateStatus(null);
				if (fSelected != text) {
					fSelected = text;
					setChanged();
				}
			} else
				updateStatus(createErrorStatus(errorText));
		}

