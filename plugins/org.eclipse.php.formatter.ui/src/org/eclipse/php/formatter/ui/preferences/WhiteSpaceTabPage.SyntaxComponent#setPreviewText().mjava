		private void setPreviewText() {
			String previewText = "";
			if (fLastSelected != null) {
				previewText = "<?php\n";
				List snippets = fLastSelected.getSnippets();
				for (int i = 0; i < snippets.size(); i++) {
					previewText += snippets.get(i);
				}
				previewText += "\n?>";
			}
			fPreview.setPreviewText(previewText);
		}

