	private ASTNode getParent(ASTNode identifier) {
		ASTNode node;
		if (identifier instanceof Identifier) {
			node = identifier.getParent();
		} else {
			node = identifier;
		}

		if (node instanceof Variable
				&& node.getParent() instanceof FunctionName) {
			return node.getParent();
		}
		return node;

	}

