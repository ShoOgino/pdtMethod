		public void run() {
			try {
				if (counter++ < 6) {
					fExecutablesCombo.getDisplay().syncExec(new Runnable() {
						public void run() {
							try {
								if (counter % 2 != 0) {
									fExecutablesCombo.setEnabled(false);
								} else {
									fExecutablesCombo.setEnabled(true);
								}
							} catch (SWTException se) {
								cancel();
							}
						}
					});
				} else {
					cancel();
				}
			} catch (SWTException se) {
				cancel();
			}
		}

