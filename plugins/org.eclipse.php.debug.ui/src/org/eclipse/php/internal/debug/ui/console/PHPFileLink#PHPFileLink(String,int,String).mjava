	/**
	 * Constructs a hyperlink to the specified file.
	 * 
	 * @param fileName
	 *            The file name to open
	 * @param lineNumber
	 *            The line number to select
	 */
	public PHPFileLink(String fileName, int lineNumber, String url) {
		this.fileName = fileName;
		this.lineNumber = lineNumber;
		this.url = url;
	}

