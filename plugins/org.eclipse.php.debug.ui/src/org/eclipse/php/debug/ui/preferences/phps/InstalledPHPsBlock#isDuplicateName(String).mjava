	/**
	 * @see IAddPHPexeDialogRequestor#isDuplicateName(String)
	 */
	public boolean isDuplicateName(String name) {
		for (int i = 0; i < fPHPexes.size(); i++) {
			PHPexeItem phpExe = (PHPexeItem) fPHPexes.get(i);
			if (phpExe.getName().equals(name)) {
				return true;
			}
		}
		return false;
	}

