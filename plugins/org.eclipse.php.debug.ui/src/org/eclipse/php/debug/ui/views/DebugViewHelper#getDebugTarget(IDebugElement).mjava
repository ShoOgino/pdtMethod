	    private PHPDebugTarget getDebugTarget (IDebugElement element){
	    	PHPDebugTarget target = null;
	    	if (element != null) {
	            if (element.getModelIdentifier().equals(IPHPConstants.ID_PHP_DEBUG_CORE)) {
	                target = (PHPDebugTarget) element.getDebugTarget();
	            }     	
	        }
	    	return target;
	    }

