    /**
     * TUM 20060608 bugfix for embedded action codes
     */
    static non_terminal create_new(String prefix, String type) throws internal_error{
        if (prefix==null) prefix = "NT$";
        return new non_terminal(prefix + next_nt++,type);
    }
