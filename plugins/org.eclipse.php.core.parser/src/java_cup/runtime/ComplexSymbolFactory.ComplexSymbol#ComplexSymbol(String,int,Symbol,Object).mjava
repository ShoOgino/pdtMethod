        public ComplexSymbol(String name, int id, Symbol left, Object value) {
            super(id,left.right,left.right,value);
            this.name=name;
            if (left!=null) { 
            	this.xleft = ((ComplexSymbol)left).xright;
            	this.xright= ((ComplexSymbol)left).xright;
            }
        }

