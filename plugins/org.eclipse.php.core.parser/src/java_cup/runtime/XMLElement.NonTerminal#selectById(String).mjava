		@Override
		public List<XMLElement> selectById(String s) {
			LinkedList<XMLElement> response= new LinkedList<XMLElement>();
			if (tagname.equals(s))
				response.add(this);
			for (XMLElement e : list){
				List<XMLElement> selection =e.selectById(s);
				response.addAll(selection);
			}
			return response;
		}

