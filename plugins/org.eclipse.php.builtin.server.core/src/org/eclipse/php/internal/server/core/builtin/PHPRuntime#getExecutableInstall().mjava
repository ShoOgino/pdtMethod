	@Override
	public PHPexeItem getExecutableInstall() {
		if (exeItem != null)
			return exeItem;
		if (getExecutableInstallName() == null)
			return null;
		try {
			String name = getExecutableInstallName();
			return PHPexes.getInstance().getItem(name);
		} catch (Exception e) {
			// ignore
		}
		return null;
	}

