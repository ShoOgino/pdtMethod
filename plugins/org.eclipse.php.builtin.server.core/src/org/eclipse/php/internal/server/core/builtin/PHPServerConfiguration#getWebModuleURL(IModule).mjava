	/**
	 * Returns the partial URL applicable to this module.
	 * 
	 * @param webModule
	 *            a web module
	 * @return the partial URL
	 */
	protected String getWebModuleURL(IModule webModule) {
		WebModule module = getWebModule(webModule);
		if (module != null) {
			return module.getPath();
		}

		return null;
	}

