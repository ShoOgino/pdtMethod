	@Override
	public boolean visit(ThrowStatement throwStatement) {
		addToLineWidth(5);
		insertSpace();
		Expression expr = throwStatement.getExpression();
		handleChars(throwStatement.getStart() + 5, expr.getStart());

		expr.accept(this);

		handleSemicolon(expr.getEnd(), throwStatement.getEnd());

		return false;
	}

