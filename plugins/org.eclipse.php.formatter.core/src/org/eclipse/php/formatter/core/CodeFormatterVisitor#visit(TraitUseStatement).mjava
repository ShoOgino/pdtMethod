	@Override
	public boolean visit(TraitUseStatement node) {
		if (node.getTraitList().size() > 0) {
			// int lastPosition = node.getStart() + 3;
			addToLineWidth(3);// the word 'use'
			insertSpace();
			handleChars(node.getStart() + 3, node.getTraitList().get(0).getStart());
		}
		return true;
	}

