		private void addToMap(final Map map, final IAdaptable key, final IWorkingSet value) {
			final Object obj = map.get(key);
			if (obj == null)
				map.put(key, value);
			else if (obj instanceof IWorkingSet) {
				final List l = new ArrayList(2);
				l.add(obj);
				l.add(value);
				map.put(key, l);
			} else if (obj instanceof List)
				((List) obj).add(value);
		}

