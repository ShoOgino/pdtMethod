	public String getText(Object element) {
		String label = super.getText(element);
		if (label != null && label.startsWith("_lzx_")) {
			label = label.substring(5);
			if (label.startsWith("Lz"))
				label = "<" + label.substring(2) + ">";
			else
				label = "<_____   id=" + label + ">";
		}

		return label;
	}

