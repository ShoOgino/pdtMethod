		public void run() {
			int numDirtyEditors = dirtyEditors.length;
			monitor.beginTask("Saving edited files", numDirtyEditors);
			for (int i = 0; i < numDirtyEditors; i++) {
				if (monitor.isCanceled()) {
					return;
				}
				dirtyEditors[i].doSave(monitor);
				monitor.worked(1);
			}
			monitor.done();
		}

