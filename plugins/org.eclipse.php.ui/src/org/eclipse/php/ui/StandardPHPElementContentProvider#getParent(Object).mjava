	public final Object getParent(Object element) {
		Object parent = null;
		if (exists(element)) {
			parent = internalGetParent(element);
		}
		if (treeProviders != null) {
			for (int i = 0; i < treeProviders.length && parent == null; i++) {
				parent = treeProviders[i].getParent(element);
			}
		}
		return parent;

	}

