		@Override
		public boolean visit(FieldAccess fieldAccess) {
			if (!fieldAccess.getField().isDollared()) {
				Expression expr = fieldAccess.getField().getName();
				if (expr instanceof Variable) {
					Variable var = (Variable) expr;
					if (!var.isDollared()) {
						highlight(var);
					}
				} else {
					highlight(expr);
				}

			}
			return true;
		}

