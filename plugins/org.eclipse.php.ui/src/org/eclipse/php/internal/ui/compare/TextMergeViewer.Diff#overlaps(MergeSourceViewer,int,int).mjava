		/*
		 * Returns true if given character range overlaps with this Diff.
		 */
		boolean overlaps(MergeSourceViewer w, int start, int end) {
			Position h= getPosition(w);
			if (h != null) {
				int ds= h.getOffset();
				int de= ds + h.getLength();
				if ((start < de) && (end >= ds))
					return true;	
			}
			return false;
		}

