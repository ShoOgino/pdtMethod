		public boolean canUpdateReferences() {
			if (getCus().length == 0)
				return false;
			IScriptFolder pack = getDestinationAsScriptFolder();
			if (pack != null && pack.isRootFolder())
				return false;
			Object commonParent = getCommonParent();
			if (ModelElementUtil.isDefaultPackage(commonParent))
				return false;
			return true;
		}

