			public void run() {
				try {
					fRunnable.run(new NullProgressMonitor());
				} catch (InvocationTargetException e) {
					fThrowable = e;
				} catch (InterruptedException e) {
					fThrowable = e;
				} catch (ThreadDeath e) {
					fThrowable = e;
					throw e;
				} catch (RuntimeException e) {
					fThrowable = e;
				} catch (Error e) {
					fThrowable = e;
				}
			}

