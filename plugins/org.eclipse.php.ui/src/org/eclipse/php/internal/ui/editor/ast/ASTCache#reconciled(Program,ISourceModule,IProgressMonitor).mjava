	public void reconciled(final Program ast, final ISourceModule input, final IProgressMonitor progressMonitor) {
		ISourceModule refInput = fInput.get();

		if (refInput == null || input != refInput) {
			return;
		}

		synchronized (this) {
			if (ast == null) {
				fState = STATES.CANCELED;
				fAST = null;
			} else {
				fState = STATES.DONE;
				fAST = ast;
			}
		}
	}

