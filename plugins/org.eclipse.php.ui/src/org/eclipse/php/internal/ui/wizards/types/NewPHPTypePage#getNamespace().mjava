	public String getNamespace() {
		if (namespaceText == null) {
			return "";//$NON-NLS-1$
		}
		if (isInFirstPHPBlock()) {
			String lastNamespace = getLastNamespace();
			if (lastNamespace != null) {
				if (!lastNamespace.equals(namespaceText.getText())) {
					return namespaceText.getText();
				} else {
					return new String();
				}
			}
		}
		return namespaceText.getText();
	}

