	private String getCurrentSettings() {
		StringBuilder buf = new StringBuilder();
		String[] names = {};
		for (int i = 0; i < names.length; i++) {
			String curr = names[i];
			buf.append(curr).append('\0');
			buf.append('\0');
		}
		return buf.toString();
	}

