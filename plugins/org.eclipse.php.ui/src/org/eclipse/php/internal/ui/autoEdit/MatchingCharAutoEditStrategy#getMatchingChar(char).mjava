	protected static char getMatchingChar(final char c) {
		switch (c) {
			case CURLY_OPEN:
				return CURLY_CLOSE;
			case CURLY_CLOSE:
				return CURLY_OPEN;
			case ROUND_OPEN:
				return ROUND_CLOSE;
			case ROUND_CLOSE:
				return ROUND_OPEN;
			case SQUARE_OPEN:
				return SQUARE_CLOSE;
			case SQUARE_CLOSE:
				return SQUARE_OPEN;
			case DOUBLE_QOUTES:
			case SINGLE_QOUTE:
			case BACK_QOUTE:
				return c;
		}

		return '-';
	}

