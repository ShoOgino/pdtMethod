	private static boolean isOperatorAssociative(int operator) {
		return operator == InfixExpression.OP_PLUS || operator == InfixExpression.OP_MUL || operator == InfixExpression.OP_CONCAT || operator == InfixExpression.OP_XOR || operator == InfixExpression.OP_OR || operator == InfixExpression.OP_AND || operator == InfixExpression.OP_STRING_AND
			|| operator == InfixExpression.OP_STRING_OR || operator == InfixExpression.OP_STRING_XOR || operator == InfixExpression.OP_BOOL_AND || operator == InfixExpression.OP_BOOL_OR;
	}

