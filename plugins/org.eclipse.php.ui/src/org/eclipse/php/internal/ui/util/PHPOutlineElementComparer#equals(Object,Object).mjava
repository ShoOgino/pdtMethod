	public boolean equals(Object a, Object b) {
		if (a == b)
			return true;
		if (a == null || b == null)
			return false;
		if (a.getClass() != b.getClass())
			return false;
		if (a.equals(b))
			return true;
		for (int i = 0; i < comparers.length; i++) {
			IPHPTreeElementComparer comparer = comparers[i];
			if (comparer.supports(a)) {
				return comparer.equals(a, b);
			}
		}
		return false;
	}

