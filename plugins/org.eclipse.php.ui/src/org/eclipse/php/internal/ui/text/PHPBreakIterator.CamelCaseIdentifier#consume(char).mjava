        /*
         * @see PHPBreakIterator.Run#consumes(char)
         */
        protected boolean consume(char ch) {
            int kind = getKind(ch);
            fState = MATRIX[fState][kind];
            switch (fState) {
                case S_LOWER:
                case S_ONE_CAP:
                case S_ALL_CAPS:
                    length++;
                    return true;
                case S_EXIT:
                    return false;
                case S_EXIT_MINUS_ONE:
                    length--;
                    return false;
                default:
                    Assert.isTrue(false);
                    return false;
            }
        }

