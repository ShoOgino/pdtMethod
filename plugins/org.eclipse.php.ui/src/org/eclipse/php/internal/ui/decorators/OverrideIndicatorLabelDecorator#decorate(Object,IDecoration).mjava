	/*
	 * (non-Javadoc)
	 * 
	 * @see
	 * org.eclipse.jface.viewers.ILightweightLabelDecorator#decorate(java.lang
	 * .Object, org.eclipse.jface.viewers.IDecoration)
	 */
	public void decorate(Object element, IDecoration decoration) {
		int adornmentFlags = computeAdornmentFlags(element);
		if ((adornmentFlags & PHPElementImageDescriptor.IMPLEMENTS) != 0) {

			decoration.addOverlay(PHPPluginImages.DESC_OVR_IMPLEMENTS);
		} else if ((adornmentFlags & PHPElementImageDescriptor.OVERRIDES) != 0) {

			decoration.addOverlay(PHPPluginImages.DESC_OVR_OVERRIDES);
		}
	}

