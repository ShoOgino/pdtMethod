	private NamespaceDeclaration getCurrentNamespace(Program program, int offset) {
		ASTNode node = program.getElementAt(offset);
		do {
			switch (node.getType()) {
				case ASTNode.NAMESPACE:
					return (NamespaceDeclaration) node;
			}
			node = node.getParent();
		} while (node != null);
		
		return null;
	}

