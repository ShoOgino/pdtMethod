	void editorActivated(IEditorPart editor) {
		if (!isLinkingEnabled())
			return;
		Object input = getElementOfInput(editor.getEditorInput());
		if (input == null)
			return;
		if (!inputIsSelected(editor.getEditorInput()))
			showInput(input);
		else
			this.fViewer.getTree().showSelection();

	}

