		public void run() {
			monitor.beginTask(PHPUIMessages.getString("SaveFilesHandler.0"), dirtyEditors.size()); //$NON-NLS-1$
			for (Iterator i = dirtyEditors.iterator(); i.hasNext();) {
				if (monitor.isCanceled()) {
					return;
				}
				((IEditorPart) i.next()).doSave(monitor);
				monitor.worked(1);
			}
			monitor.done();
		}

