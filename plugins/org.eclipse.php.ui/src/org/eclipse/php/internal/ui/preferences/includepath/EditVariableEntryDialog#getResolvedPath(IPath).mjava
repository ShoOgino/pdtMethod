	private IPath getResolvedPath(IPath path) {
		if (path != null) {
			String varName = path.segment(0);
			if (varName != null) {
				IPath varPath = PHPProjectOptions.getIncludePathVariable(varName);
				if (varPath != null) {
					return varPath.append(path.removeFirstSegments(1));
				}
			}
		}
		return null;
	}

