	ITypedElement getLeg(char type, Object input) {
		if (input instanceof ICompareInput) {
			switch (type) {
				case 'A':
					return ((ICompareInput) input).getAncestor();
				case 'L':
					return ((ICompareInput) input).getLeft();
				case 'R':
					return ((ICompareInput) input).getRight();
			}
		}
		return null;
	}

