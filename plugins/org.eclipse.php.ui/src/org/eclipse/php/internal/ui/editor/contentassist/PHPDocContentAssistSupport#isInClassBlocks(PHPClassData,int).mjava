	private CodeData isInClassBlocks(PHPClassData data, int position) {
		if (isInBlock(data, position)) {
			return data;
		} else {
			PHPFunctionData[] functions = data.getFunctions();
			for (PHPFunctionData element : functions) {
				if (isInBlock(element, position)) {
					return element;
				}
			}
		}
		return null;
	}

