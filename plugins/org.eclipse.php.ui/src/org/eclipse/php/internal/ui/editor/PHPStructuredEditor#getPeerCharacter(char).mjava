	private static char getPeerCharacter(char character) {
		switch (character) {
		case '(':
			return ')';
		case ')':
			return '(';
		case '[':
			return ']';
		case ']':
			return '[';
		case '"':
			return character;
		case '\'':
			return character;
		case '`':
			return character;
		default:
			throw new IllegalArgumentException();
		}
	}

