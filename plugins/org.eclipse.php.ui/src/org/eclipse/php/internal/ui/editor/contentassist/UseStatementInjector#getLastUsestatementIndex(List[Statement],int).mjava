	private int getLastUsestatementIndex(List<Statement> statements, int offset) {
		int result = 0;
		for (int i = 0; i < statements.size(); i++) {
			Statement statement = statements.get(i);
			if (statement.getEnd() <= offset
					&& statement instanceof UseStatement) {
				result = i + 1;
			}
		}
		return result;
	}

