	@Override
	public boolean equals(Object obj) {
		if (!(obj instanceof ASTCache)) {
			return false;
		}
		ISourceModule thisInput = fInput.get();
		ISourceModule otherInput = ((ASTCache) obj).fInput.get();

		if (thisInput == null || otherInput == null || thisInput != otherInput) {
			return false;
		}

		return true;
	}

