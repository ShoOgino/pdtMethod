		@Override
		public boolean visit(Scalar scalar) {
			String value = scalar.getStringValue();
			if ((scalar.getScalarType() == Scalar.TYPE_STRING || scalar
					.getScalarType() == Scalar.TYPE_SYSTEM)
					&& !"null".equals(value)
					&& !"false".equals(value)
					&& !"true".equals(value)
					&& value.charAt(0) != '\''
					&& value.charAt(0) != '"') {
				highlight(scalar);
			}
			return true;
		}

