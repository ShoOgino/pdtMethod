	/**
	 * Disposes the cached AST.
	 */
	private synchronized void disposeAST() {

		if (fAST == null) {
			return;
		}

		if (DEBUG) {
			System.out.println(getThreadName() + " - " + DEBUG_PREFIX + "disposing AST: " + toString(fAST) + " for: " //$NON-NLS-1$ //$NON-NLS-2$ //$NON-NLS-3$
					+ toString(fActiveJavaElement));
		}
		fAST = null;

		cache(null, null);
	}

