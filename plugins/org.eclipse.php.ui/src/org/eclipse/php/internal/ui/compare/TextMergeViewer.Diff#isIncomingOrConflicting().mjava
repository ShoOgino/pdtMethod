		private boolean isIncomingOrConflicting() {
			switch (fDirection) {
				case RangeDifference.RIGHT:
					if (fLeftIsLocal)
						return true;
					break;
				case RangeDifference.LEFT:
					if (!fLeftIsLocal)
						return true;
					break;
				case RangeDifference.CONFLICT:
					return true;
			}
			return false;
		}

