		protected String resolve(TemplateContext context) {
			String path = null;

			try {
				ISourceModule module = getSourceModule(context);
				if (module == null)
					return null;

				IFile file = (IFile) module.getResource();
				return file.getCharset();

			} catch (CoreException e) {
				PHPUiPlugin.log(e);
			}

			return path;
		}

