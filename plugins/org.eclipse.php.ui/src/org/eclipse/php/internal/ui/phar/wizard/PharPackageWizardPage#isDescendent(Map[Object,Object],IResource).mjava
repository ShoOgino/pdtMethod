	/**
	 * Answer a boolean indicating whether the passed child is a descendent of one
	 * or more members of the passed resources collection
	 * 
	 * @return boolean
	 * @param resources
	 *            java.util.Vector
	 * @param child
	 *            org.eclipse.core.resources.IResource
	 */
	protected boolean isDescendent(Map<Object, Object> resources, IResource child) {
		if (child.getType() == IResource.PROJECT) {
			return false;
		}

		IResource parent = child.getParent();
		if (resources.containsKey(parent)) {
			return true;
		}

		return isDescendent(resources, parent);
	}

