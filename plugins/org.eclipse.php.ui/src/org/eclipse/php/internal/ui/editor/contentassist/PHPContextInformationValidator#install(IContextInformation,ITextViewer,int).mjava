	public void install(IContextInformation info, ITextViewer viewer, int offset) {
		fInformation = info;
		fViewer = viewer;
		if (info instanceof IContextInformationExtension) {
			fPosition = ((IContextInformationExtension) info).getContextInformationPosition();
		} else {
			fPosition = offset;
		}
		fCurrentParameter = -1;
	}

