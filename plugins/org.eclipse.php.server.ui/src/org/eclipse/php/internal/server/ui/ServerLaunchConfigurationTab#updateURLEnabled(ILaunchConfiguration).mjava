	private void updateURLEnabled(ILaunchConfiguration configuration)
			throws CoreException {
		boolean enabled = configuration.getAttribute(SERVER_ENABLED, true);
		boolean autoGenerateURL = autoGeneratedURL.getSelection();
		fURLLabel.setEnabled(!autoGenerateURL && enabled);
		fURLPath.setEnabled(!autoGenerateURL && enabled);
		autoGeneratedURL.setEnabled(enabled);
	}

