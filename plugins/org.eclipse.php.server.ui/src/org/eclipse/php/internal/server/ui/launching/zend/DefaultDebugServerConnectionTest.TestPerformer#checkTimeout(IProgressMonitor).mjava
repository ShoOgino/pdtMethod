		private boolean checkTimeout(IProgressMonitor monitor)
				throws InterruptedException {
			for (int i = 0; i < 10; i++) {
				// Go out if cancelled
				if (monitor.isCanceled())
					return false;
				Thread.sleep(DEFAULT_TIMEOUT / 10);
				if (fIsFinished) {
					return false;
				}
			}
			return true;
		}

