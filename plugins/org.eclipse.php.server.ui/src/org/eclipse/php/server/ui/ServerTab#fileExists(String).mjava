	private boolean fileExists(String projectPath) {
		if (projectPath == null || "".equals(projectPath)) {
			return false;
		}
		boolean file = false;
		try {
			IWorkspaceRoot root = ResourcesPlugin.getWorkspace().getRoot();
			IPath p3 = new Path(projectPath);

			file = root.exists(p3);
		} catch (Exception e) {
		}

		return file;
	}

