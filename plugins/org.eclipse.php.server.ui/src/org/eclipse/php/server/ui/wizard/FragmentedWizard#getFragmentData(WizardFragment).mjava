	private FragmentedWizardPage getFragmentData(WizardFragment fragment) {
		try {
			FragmentedWizardPage page = (FragmentedWizardPage) fragmentData.get(fragment);
			if (page != null)
				return page;
		} catch (Exception e) {
			Logger.logException("Error getting fragment data", e); //$NON-NLS-1$
		}

		return null;
	}

