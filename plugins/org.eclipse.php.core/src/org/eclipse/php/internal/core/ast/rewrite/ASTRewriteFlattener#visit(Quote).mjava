	public boolean visit(Quote quote) {
		switch (quote.getQuoteType()) {
			case 0:
				result.append("\""); //$NON-NLS-1$
				acceptQuoteExpression(quote.getExpressions());
				result.append("\""); //$NON-NLS-1$
				break;
			case 1:
				result.append("\'"); //$NON-NLS-1$
				acceptQuoteExpression(quote.getExpressions());
				result.append("\'"); //$NON-NLS-1$
				break;
			case 2:
				result.append("<<<Heredoc\n"); //$NON-NLS-1$
				acceptQuoteExpression(quote.getExpressions());
				result.append("\nHeredoc"); //$NON-NLS-1$
		}
		return false;
	}

