	public void traverse(ASTVisitor pVisitor) throws Exception {
		if( pVisitor.visit( this ) ) {
			if( receiver != null ) {
				receiver.traverse( pVisitor );
			}
			getCallName().traverse(pVisitor);
			if(getArgs() != null ) {
				getArgs().traverse( pVisitor );
			}
			pVisitor.endvisit( this );
		}
	}

