	public static String getType(int type) {
		switch (type) {
			case TYPE_INT:
				return "int";
			case TYPE_REAL:
				return "real";
			case TYPE_STRING:
				return "string";
			case TYPE_UNKNOWN:
				return "unknown";
			case TYPE_SYSTEM:
				return "system";
			default:
				throw new IllegalArgumentException();
		}
	}

