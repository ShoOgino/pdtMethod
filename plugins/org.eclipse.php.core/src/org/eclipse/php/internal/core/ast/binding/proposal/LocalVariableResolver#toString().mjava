  public String toString()
  {
    String r = "LocalVariableResolver: "+super.toString()+" "+(_indeterminate?"INDETERMINATE":"determinate")+" ";
    r+= "Globals(";
    for (final String g:_globals)
      {
        r+=g+",";
      }
    r+=") Undetermined (";
    for (final String u:_undetermined)
      {
        r += u+",";
      }
    return r+")";
  }

