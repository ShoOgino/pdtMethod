	public IGoal[] init() {
		ExpressionTypeGoal typedGoal = (ExpressionTypeGoal) goal;
		ConditionalExpression conditionalExpression = (ConditionalExpression) typedGoal
				.getExpression();
		return new IGoal[] {
				new ExpressionTypeGoal(goal.getContext(), conditionalExpression
						.getIfTrue()),
				new ExpressionTypeGoal(goal.getContext(), conditionalExpression
						.getIfFalse()) };
	}

