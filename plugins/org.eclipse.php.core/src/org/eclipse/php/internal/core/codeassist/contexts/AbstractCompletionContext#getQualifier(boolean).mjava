	public String getQualifier(boolean useGlobal) throws BadLocationException {
		if (isAbsoluteName()) {
			if (resolvedNamespaceName == null && useGlobal) {
				return PHPCoreConstants.GLOBAL_NAMESPACE;
			} else {
				return resolvedNamespaceName;
			}
		} else {
			return resolvedNamespaceName;
		}
	}

