	/**
	 * @param ASTNode
	 *            the node Checks if the node is in the offset if true then
	 *            assign the node to the children' nodes
	 */
	@Override
	public boolean apply(ASTNode node) {
		assert node != null;

		if (inNode(node, offset)) {
			this.currentNode = node;
			return true;
		}

		return false;
	}

