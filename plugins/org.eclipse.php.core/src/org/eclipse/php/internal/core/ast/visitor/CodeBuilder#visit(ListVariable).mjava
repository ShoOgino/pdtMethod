	public void visit(ListVariable listVariable) {
		buffer.append("list(");
		boolean isFirst = true;
		VariableBase[] variables = listVariable.getVariables();
		for (int i = 0; i < variables.length; i++) {
			if (!isFirst) {
				buffer.append(", ");
			}
			variables[i].accept(this);
			isFirst = false;
		}
		buffer.append(")");
	}

