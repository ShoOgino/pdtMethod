	public void toString(StringBuffer buffer, String tab) {
		buffer.append(tab).append("<ParenthesisExpression");
		appendInterval(buffer);
		buffer.append(">\n");
		if (expr != null) {
			expr.toString(buffer, TAB + tab);
			buffer.append("\n");
		}
		buffer.append(tab).append("</ParenthesisExpression>");
	}

