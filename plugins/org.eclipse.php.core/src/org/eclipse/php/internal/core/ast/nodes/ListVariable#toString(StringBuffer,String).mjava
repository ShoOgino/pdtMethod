	public void toString(StringBuffer buffer, String tab) {
		buffer.append(tab).append("<List");
		appendInterval(buffer);
		buffer.append(">\n");
		for (int i = 0; i < variables.length; i++) {
			variables[i].toString(buffer, TAB + tab);
			buffer.append("\n");
		}
		buffer.append(tab).append("</List>");
	}

