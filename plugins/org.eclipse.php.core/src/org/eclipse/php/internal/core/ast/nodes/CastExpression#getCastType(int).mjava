	public static String getCastType(int type) {
		switch (type) {
			case TYPE_INT:
				return "int";
			case TYPE_REAL:
				return "real";
			case TYPE_STRING:
				return "string";
			case TYPE_ARRAY:
				return "array";
			case TYPE_OBJECT:
				return "object";
			case TYPE_BOOL:
				return "bool";
			case TYPE_UNSET:
				return "unset";
			default:
				throw new IllegalArgumentException();
		}
	}

