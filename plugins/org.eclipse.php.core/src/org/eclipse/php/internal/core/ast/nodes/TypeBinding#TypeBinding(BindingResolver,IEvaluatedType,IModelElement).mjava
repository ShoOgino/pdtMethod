	/**
	 * Constructs a new TypeBinding.
	 * 
	 * @param resolver
	 * @param type
	 * @param element
	 */
	public TypeBinding(BindingResolver resolver, IEvaluatedType type, IModelElement element) {
		this.resolver = resolver;
		this.type = type;
		if (element != null) {
			this.elements = new IModelElement[] { element };
		}
	}

