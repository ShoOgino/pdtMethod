	public InfixExpression(int start, int end, AST ast, Expression left, int operator, Expression right) {
		super(start, end, ast);

		if (right == null || left == null || getOperator(operator) == null) {
			throw new IllegalArgumentException();
		}
		
		setLeft(left);
		setOperator(operator);
		setRight(right);
	}

