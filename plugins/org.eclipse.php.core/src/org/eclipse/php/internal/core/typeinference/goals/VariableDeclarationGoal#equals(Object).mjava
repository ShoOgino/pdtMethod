	public boolean equals(Object obj) {
		if (this == obj) {
			return true;
		}
		if (obj == null) {
			return false;
		}
		if (getClass() != obj.getClass()) {
			return false;
		}
		VariableDeclarationGoal other = (VariableDeclarationGoal) obj;
		if (declaration == null) {
			if (other.declaration != null) {
				return false;
			}
		} else if (!declaration.equals(other.declaration)) {
			return false;
		}
		return true;
	}

