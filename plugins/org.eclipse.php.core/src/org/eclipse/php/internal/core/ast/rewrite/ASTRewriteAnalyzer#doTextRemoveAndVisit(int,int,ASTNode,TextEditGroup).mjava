	final void doTextRemoveAndVisit(int offset, int len, ASTNode node, TextEditGroup editGroup) {
		TextEdit edit = doTextRemove(offset, len, editGroup);
		if (edit != null) {
			this.currentEdit = edit;
			voidVisit(node);
			this.currentEdit = edit.getParent();
		} else {
			voidVisit(node);
		}
	}

