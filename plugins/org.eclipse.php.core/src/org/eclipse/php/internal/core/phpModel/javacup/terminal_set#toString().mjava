  /** Convert to string. */
  public String toString()
    {
      String result;
      boolean comma_flag;
      
      result = "{"; //$NON-NLS-1$
      comma_flag = false;
      for (int t = 0; t < terminal.number(); t++)
	{
	  if (_elements.get(t))
	    {
	      if (comma_flag)
	        result += ", "; //$NON-NLS-1$
	      else
	        comma_flag = true;

	      result += terminal.find(t).name();
	    }
	}
      result += "}"; //$NON-NLS-1$

      return result;
    }

