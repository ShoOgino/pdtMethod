	public GlobalInterfacesStrategy() {
		super(new IElementFilter() {
			public boolean filter(IModelElement element) {
				try {
					return !PHPFlags.isInterface(((IType)element).getFlags());
				} catch (ModelException e) {
					if (DLTKCore.DEBUG_COMPLETION) {
						e.printStackTrace();
					}
				}
				return false;
			}
		});
	}

