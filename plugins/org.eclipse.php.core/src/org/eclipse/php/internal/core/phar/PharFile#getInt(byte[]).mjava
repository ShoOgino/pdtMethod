	public static int getInt(byte[] subBytes) {
		int result = 0;
		if (subBytes.length > 0) {
			result = PharUtil.getPositive(subBytes[subBytes.length - 1]);
			for (int i = 0; i < subBytes.length - 1; i++) {
				result = result
						* 256
						+ PharUtil
								.getPositive(subBytes[subBytes.length - 2 - i]);
			}
		}
		return result;
	}

