	public String toString() {
		StringBuilder buf = new StringBuilder("[USE: ").append(getNamespace().getFullyQualifiedName()); //$NON-NLS-1$
		if (getAlias() != null) {
			buf.append(" AS ").append(getAlias().getName()); //$NON-NLS-1$
		}
		buf.append("]"); //$NON-NLS-1$
		return buf.toString();
	}

