		public String[] getReturnTypes() {
			Map<String, String> info = decodeDocInfo(doc);
			if (info != null) {
				String types = info.get("r");
				if (types != null) {
					String[] returnTypes = types.split(",");
					for (int i = 0; i < returnTypes.length; i++) {
						returnTypes[i] = returnTypes[i].replaceAll("~", ",");
					}
					return returnTypes;
				}
			}
			return null;
		}

