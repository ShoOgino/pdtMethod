	/**
	 * Creates global namespace node (without name)
	 * @param start
	 * @param end
	 * @param ast
	 * @param body
	 */
	public Namespace(int start, int end, AST ast, Block body) {
		super(start, end, ast);

		// Body must be set in this case
		if (body == null) {
			throw new IllegalArgumentException();
		}
	}

