		private void handleProjectRename(IProject from, IProject to) {
			if (from == project) {
				handleProjectDeletion(from);
			} else {
				removeProject(from);
				addProject(to);

				PHPProjectOptions options = PHPProjectOptions.forProject(project);
				if (options == null)
					return;

				options.renameResourceAtIncludePath(from, to);
			}
		}

