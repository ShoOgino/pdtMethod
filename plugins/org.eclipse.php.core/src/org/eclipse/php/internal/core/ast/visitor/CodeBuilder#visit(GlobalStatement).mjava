	public void visit(GlobalStatement globalStatement) {
		buffer.append("global ");
		boolean isFirst = true;
		Variable[] variables = globalStatement.getVariables();
		for (int i = 0; i < variables.length; i++) {
			if (!isFirst) {
				buffer.append(", ");
			}
			variables[i].accept(this);
			isFirst = false;
		}
		buffer.append(";\n ");
	}

