	public void toString(StringBuffer buffer, String tab) {
		buffer.append(tab).append("<ArrayCreation");
		appendInterval(buffer);
		buffer.append(">\n");
		for (int i = 0; i < elements.length; i++) {
			elements[i].toString(buffer, TAB + tab);
			buffer.append("\n");
		}
		buffer.append(tab).append("</ArrayCreation>");
	}

