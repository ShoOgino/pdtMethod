	public static String getType(int type) {
		switch (type) {
			case QT_QUOTE:
				return "quote"; //$NON-NLS-1$
			case QT_SINGLE:
				return "single"; //$NON-NLS-1$
			case QT_HEREDOC:
				return "heredoc"; //$NON-NLS-1$
			case QT_NOWDOC:
				return "nowdoc"; //$NON-NLS-1$
			default:
				throw new IllegalArgumentException();
		}
	}

