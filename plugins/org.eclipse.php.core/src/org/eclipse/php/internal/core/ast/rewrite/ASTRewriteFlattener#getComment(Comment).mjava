	public String getComment(Comment comment) {
		if (comment.getCommentType() == Comment.TYPE_SINGLE_LINE) {
			return "//";
		}
		if (comment.getCommentType() == Comment.TYPE_MULTILINE) {
			return "/* */";
		}
		if (comment.getCommentType() == Comment.TYPE_PHPDOC) {
			return "/** */"; //$NON-NLS-1$"
		}
		return null;
	}

