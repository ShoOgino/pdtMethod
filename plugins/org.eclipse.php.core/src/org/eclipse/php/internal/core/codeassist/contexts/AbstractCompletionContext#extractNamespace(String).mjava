	protected String extractNamespace(String prefix) {
		prefix = realPrefix(prefix);

		int pos = prefix.lastIndexOf(NamespaceReference.NAMESPACE_DELIMITER);
		if (pos == -1) {
			return null;
		}

		String name = prefix.substring(0, pos);
		if (name.length() == 0) {
			return null;
		}

		return name;
	}

