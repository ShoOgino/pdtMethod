	private static String getExtension(File localFile) {
		if (localFile.isFile()) {
			int index = localFile.getName().lastIndexOf("."); //$NON-NLS-1$
			if (localFile.getName().length() > index + 1) {
				String extension = localFile.getName().substring(index + 1);
				return extension;
			}

		}
		return null;
	}

