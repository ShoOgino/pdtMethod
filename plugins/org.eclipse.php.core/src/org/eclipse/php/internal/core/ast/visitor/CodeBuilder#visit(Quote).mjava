	public void visit(Quote quote) {
		switch (quote.getQuoteType()) {
			case 0:
				buffer.append("\"");
				acceptQuoteExpression(quote.getExpressions());
				buffer.append("\"");
				break;
			case 1:
				buffer.append("\'");
				acceptQuoteExpression(quote.getExpressions());
				buffer.append("\'");
				break;
			case 2:
				buffer.append("<<<Heredoc\n");
				acceptQuoteExpression(quote.getExpressions());
				buffer.append("\nHeredoc");
		}
	}

