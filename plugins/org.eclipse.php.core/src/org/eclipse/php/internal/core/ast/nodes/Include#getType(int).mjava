	public static String getType(int type) {
		switch (type) {
			case IT_REQUIRE:
				return "require";
			case IT_REQUIRE_ONCE:
				return "require_once";
			case IT_INCLUDE:
				return "include";
			case IT_INCLUDE_ONCE:
				return "include_once";
			default:
				throw new IllegalArgumentException();
		}
	}

