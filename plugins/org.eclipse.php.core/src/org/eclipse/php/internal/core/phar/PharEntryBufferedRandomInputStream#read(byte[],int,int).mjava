	@Override
	public int read(byte[] b, int off, int len) throws IOException {
		// if(flag){
		int readLength = totalLength - currentIndex;
		if (readLength <= 0)
			return -1;
		if (readLength < len) {
			len = readLength;
		} else {
			readLength = len;
		}
		currentIndex = currentIndex + readLength;
		int result = bufferedRandomInputStream.read(b, off, len);
		return result;
	}

