	private int getIndexOf(File library, ArrayList libraries) {
		int index = 0;
		String libraryName = library.getPath();
		Iterator iter = libraries.iterator();

		while (iter.hasNext()) {
			File tmp = (File) iter.next();
			if (tmp.getPath().equals(libraryName)) {
				return index;
			}
			index++;
		}
		return -1;
	}

