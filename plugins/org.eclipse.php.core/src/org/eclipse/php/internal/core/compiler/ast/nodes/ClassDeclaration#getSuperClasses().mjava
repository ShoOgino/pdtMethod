	public ASTListNode getSuperClasses() {
		ASTListNode listNode = new ASTListNode(sourceStart(), sourceEnd());
		if (superClass != null) {
			listNode.addNode(superClass);
		}
		if (interfaceList != null) {
			for (TypeReference iface : interfaceList) {
				listNode.addNode(iface);
			}
		}
		return listNode;
	}

