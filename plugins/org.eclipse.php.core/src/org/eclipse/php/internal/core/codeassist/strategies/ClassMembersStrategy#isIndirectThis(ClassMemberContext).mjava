	/**
	 * class A { static private $var = 0; function foo() { $a = new A(); $a->| } }
	 * access level as for 'this' keyword in this case.
	 * 
	 * @param context
	 * @return
	 */
	private boolean isIndirectThis(ClassMemberContext context) {
		return isThisCall(context) && !isDirectThis(context);
	}

