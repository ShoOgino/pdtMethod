	boolean internalGetSetBooleanProperty(SimplePropertyDescriptor property, boolean get, boolean value) {
		if (property == BRACKETED_PROPERTY) {
			if (get) {
				return isBracketed();
			} else {
				setBracketed(value); 
				return false;
			}
		}
		return super.internalGetSetBooleanProperty(property, get, value);
	}

