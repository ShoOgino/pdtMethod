		@Override
		public String getType() throws ModelException {
			Map<String, String> info = decodeDocInfo(doc);
			if (info != null) {
				String types = info.get("v"); //$NON-NLS-1$
				types = types.replace(Constants.DOT, Constants.TYPE_SEPERATOR_CHAR);
				return types;
			}
			return null;
		}

