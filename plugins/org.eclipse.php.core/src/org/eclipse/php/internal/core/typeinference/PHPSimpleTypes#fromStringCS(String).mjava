	/**
	 * Case sensitive version
	 * 
	 * @param type
	 * @return
	 * @since 3.5
	 * @deprecated
	 * @see PHPSimpleTypes#fromString(String)
	 */
	@Nullable
	public static IEvaluatedType fromStringCS(@Nullable String type) {
		if (type == null) {
			return null;
		}
		TypeInfo typeInfo = SIMPLE_TYPES.get(type);
		if (typeInfo != null) {
			return typeInfo.type;
		}

		return null;
	}

