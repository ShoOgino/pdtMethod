	public boolean visit(UseStatement useStatement) {
		result.append("use "); //$NON-NLS-1$
		Iterator<UseStatementPart> it = useStatement.parts().iterator();
		while (it.hasNext()) {
			it.next().accept(this);
			if (it.hasNext()) {
				result.append(", "); //$NON-NLS-1$
			}
		}
		result.append(";\n"); //$NON-NLS-1$
		return false;
	}

