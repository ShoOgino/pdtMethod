		public LinkedList<ASTNode> getDeclarations() {
			int nullIdx;
			// Remove all outer level declarations
			for (int i = 0; i < variableLevel; ++i) {
				declarations.set(i, null);
			}
			while ((nullIdx = declarations.indexOf(null)) != -1) {
				declarations.remove(nullIdx);
			}
			return declarations;
		}

