	public static String getModifier(int modifier) {
		switch (modifier) {
			case MODIFIER_NONE:
				return ""; //$NON-NLS-1$
			case MODIFIER_ABSTRACT:
				return "abstract"; //$NON-NLS-1$
			case MODIFIER_FINAL:
				return "final"; //$NON-NLS-1$
			default:
				throw new IllegalArgumentException();
		}
	}

