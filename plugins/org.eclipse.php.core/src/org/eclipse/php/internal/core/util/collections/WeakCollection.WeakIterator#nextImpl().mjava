		/**
		 * This method gets the next non null object from the referenced
		 * collection.
		 */
		private Object nextImpl() {
			Object referant;
			while (referencedIterator.hasNext()) {
				referant = ((WeakObject) referencedIterator.next()).get();
				if (referant != null) {
					return referant;
				}
				referencedIterator.remove();
			}
			return null;
		}

