	public String getSuffix(AbstractCompletionContext abstractContext,
			int currentPosition) {
		char nextWord = ' ';
		try {
			nextWord = abstractContext.getNextChar();
		} catch (BadLocationException e) {
			PHPCorePlugin.log(e);
		}
		return '(' == nextWord ? "" : "()"; //$NON-NLS-1$ //$NON-NLS-2$ //$NON-NLS-3$
	}

