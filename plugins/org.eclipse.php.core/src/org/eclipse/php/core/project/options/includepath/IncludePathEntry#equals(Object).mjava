	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		final IncludePathEntry other = (IncludePathEntry) obj;
		if (contentKind != other.contentKind)
			return false;
		if (entryKind != other.entryKind)
			return false;
		if (path == null) {
			if (other.path != null)
				return false;
		} else if (!path.equals(other.path))
			return false;
		return true;
	}

