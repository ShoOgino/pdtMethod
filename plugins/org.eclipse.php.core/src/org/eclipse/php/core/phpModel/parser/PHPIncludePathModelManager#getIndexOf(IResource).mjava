	private int getIndexOf(IResource project) {
		int index = 0;
		String projectName = project.getName();
		Iterator iter = projects.iterator();

		while (iter.hasNext()) {
			IResource tmp = (IResource) iter.next();
			if (tmp.getName().equals(projectName)) {
				return index;
			}
			index++;
		}
		return -1;
	}

