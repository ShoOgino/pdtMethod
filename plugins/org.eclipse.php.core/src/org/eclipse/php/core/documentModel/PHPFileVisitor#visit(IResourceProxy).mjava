	public boolean visit(IResourceProxy proxy) throws CoreException {

		// check validation
		if (fReporter.isCancelled())
			return false;

		if (proxy.getType() == IResource.FILE) {
			IFile file = (IFile) proxy.requestResource();
			if (file.exists()) {
				if (canHandle(file)) {
					fFiles.add(file);
					// don't search deeper for files
					return false;
				}
			}
		}
		return true;
	}

