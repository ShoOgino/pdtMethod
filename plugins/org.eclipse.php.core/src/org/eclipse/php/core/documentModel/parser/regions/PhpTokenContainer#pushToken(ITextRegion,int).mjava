	/**
	 * Push region to the end of the tokens list
	 * @param region
	 * @param lexerState
	 */
	public void pushToken(ITextRegion region, int lexerState) {
		assert region != null;

		if (phpTokens.size() % DIV_DENOMINATOR == 0) {
			lexerStates.add(new Integer(lexerState));
		}

		phpTokens.add(region);
	}

