    public boolean addAll(Collection c) {
        boolean modified = false;
        if (c instanceof WeakCollection) {
            modified = refrencedCollection.addAll(((WeakCollection)c).refrencedCollection);
        } else {
            Iterator it = c.iterator();
            while (it.hasNext()) {
                if (add(it.next()))
                    modified = true;
            }
        }
        return modified;
    }

