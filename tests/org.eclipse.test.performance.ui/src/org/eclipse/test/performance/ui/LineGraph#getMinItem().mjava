    public double getMinItem() {
        double minItem= getMaxItem();
        for (int i= 0; i < this.fItems.size(); i++) {
            GraphItem graphItem= (GraphItem) this.fItems.get(i);
            if (graphItem.value < minItem)
                minItem= graphItem.value;
        }
        if (minItem == 0)
            return -1;
        return minItem;
    }

