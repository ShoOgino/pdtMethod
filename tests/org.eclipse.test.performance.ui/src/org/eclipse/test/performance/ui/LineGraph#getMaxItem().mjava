    public double getMaxItem() {
        double maxItem= 0;
        for (int i= 0; i < this.fItems.size(); i++) {
            GraphItem graphItem= this.fItems.get(i);
            if (graphItem.value > maxItem)
                maxItem= graphItem.value;
        }
        if (maxItem == 0)
            return 1;
        return maxItem;
    }

