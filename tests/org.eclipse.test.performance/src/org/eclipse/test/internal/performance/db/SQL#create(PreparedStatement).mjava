	static int create(PreparedStatement stmt) throws SQLException {
		stmt.executeUpdate();
		ResultSet rs= stmt.getGeneratedKeys();
		if (rs != null) {
			try {
				if (rs.next()) {
					BigDecimal idColVar= rs.getBigDecimal(1);
					return idColVar.intValue();
				}
			} finally {
				rs.close();
			}
		}
		return 0;
	}

