	@Override
	public Sample getSample() {
	    if (fDataPoints != null)
	        return new Sample(getScenarioName(), fStartTime, new HashMap<>(), fDataPoints.toArray(new DataPoint[fDataPoints.size()]));
	    return null;
	}

