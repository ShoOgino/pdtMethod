	private static IPath getResourceFolder(String fileName) {
		IPath path = null;
		try {
			path = new Path(FileLocator.getBundleFile(
					PHPCoreTests.getDefault().getBundle()).getAbsolutePath())
					.append(fileName);
		} catch (IOException e) {
			fail(e.getLocalizedMessage());
		}
		return path;
	}

