	@After
	public void after() throws Exception {
		if (testFile != null) {
			PHPCoreTests.removeIndex(testFile);
			testFile.delete(true, null);
			testFile = null;
		}
		if (otherFiles != null) {
			for (IFile file : otherFiles) {
				if (file != null) {
					PHPCoreTests.removeIndex(file);
					file.delete(true, null);
				}
			}
			otherFiles = null;
		}
	}

