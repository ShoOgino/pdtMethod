	protected void writeStates(PrintWriter w) {
		w.println("--TEST--");
		w.println(description);
		if (config != null) {
			w.println("--CONFIG--");
			for (String key : config.keySet()) {
				w.print(key);
				w.print(':');
				w.println(config.get(key));
			}
		}
		w.println("--FILE--");
		w.println(file);
		w.println("--EXPECT--");
		w.println(expected);
	}

