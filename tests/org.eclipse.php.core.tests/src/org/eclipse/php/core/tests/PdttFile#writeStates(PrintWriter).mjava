	protected void writeStates(PrintWriter w) {
		w.println("--TEST--");
		w.println(description.trim());
		if (config != null && config.size() > 0) {
			w.println("--CONFIG--");
			for (String key : config.keySet()) {
				w.print(key);
				w.print(':');
				w.println(config.get(key));
			}
		}
		w.println("--FILE--");
		w.println(file.trim());
		w.println("--EXPECT--");
		w.println(expected.trim());
	}

