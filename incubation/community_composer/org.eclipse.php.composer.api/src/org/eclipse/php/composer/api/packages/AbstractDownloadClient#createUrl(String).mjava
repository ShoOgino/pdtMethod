	protected String createUrl(String param) {
		try {
			if (baseUrlParamEncoding) {
				param = URLEncoder.encode(param, "UTF-8");
			}
			String url = String.format(baseUrl, param);
			
			if (filter != null) {
				if (url.contains("?")) {
					url += "&type=" + filter;
				} else {
					url += "?type=" + filter;
				}
			}
			
			return url;
		} catch (UnsupportedEncodingException e) {
			log.error(e);
		}
		return null;
	}

