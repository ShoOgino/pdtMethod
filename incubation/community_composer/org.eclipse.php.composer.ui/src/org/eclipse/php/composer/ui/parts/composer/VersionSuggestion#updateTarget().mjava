	private void updateTarget() {
		updatingTarget = true;
		if (lastUpdate == RECENT
				|| (version.getMajor() != null && !version.getMajor().isEmpty() && version.getMajor() != "null")) {

			String v = version.toString();
			if (v != null) {
				target.setText(v);
			}
		}

		updatingTarget = false;
	}

