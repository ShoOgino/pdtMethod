		@Override
		protected IStatus run(IProgressMonitor monitor) {
			while (cmd.process != null) {
				if (toMonitor.isCanceled()) {
					cmd.process.destroy();
					break;
				} else {
					try {
						Thread.sleep(200);
					} catch (InterruptedException e) {
						ComposerCorePlugin.logError(e);
					}
				}
			}
			return org.eclipse.core.runtime.Status.OK_STATUS;
		}

