	public void serialize() {
		try {
			ObjectMapper objectMapper = createObjectMapper();
			store.getOutput().write(objectMapper.writeValueAsString(model));
			store.write();
		} catch (CoreException e) {
			ComposerCorePlugin.logError(e);
		} catch (IOException e) {
			ComposerCorePlugin.logError(e);
		}
	}

