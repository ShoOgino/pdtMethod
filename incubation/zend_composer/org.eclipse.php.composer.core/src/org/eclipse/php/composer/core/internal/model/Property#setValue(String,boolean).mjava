	@Override
	public void setValue(String value, boolean update) {
		if ((value != null && !value.equals(this.value))
				|| (this.value != null && value == null)) {
			if (value != null && value.isEmpty() && this.value == null) {
				return;
			}
			this.value = value;
			if (update) {
				updateModel();
			}
		}
	}

