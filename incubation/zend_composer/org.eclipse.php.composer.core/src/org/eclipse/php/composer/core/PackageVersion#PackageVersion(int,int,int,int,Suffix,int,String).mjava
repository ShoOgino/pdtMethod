	private PackageVersion(int major, int minor, int build, int revision,
			Suffix suffix, int suffixVersion, String fullVersion) {
		this.major = major;
		this.minor = minor;
		this.build = build;
		this.revision = revision;
		this.suffix = suffix;
		this.suffixVersion = suffixVersion;
		this.fullVersion = fullVersion;
	}

