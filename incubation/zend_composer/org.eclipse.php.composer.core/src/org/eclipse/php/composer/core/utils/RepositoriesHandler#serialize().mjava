	private void serialize() {
		try {
			ComposerJacksonMapper.getMapper().writeValue(composerFile, composerRoot);
		} catch (JsonGenerationException e) {
			ComposerCorePlugin.logError(e);
		} catch (JsonMappingException e) {
			ComposerCorePlugin.logError(e);
		} catch (IOException e) {
			ComposerCorePlugin.logError(e);
		}
	}

